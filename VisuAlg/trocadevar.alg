algoritmo "semnome"
// Função :
// Autor :
// Data : 04/07/2018
// Seção de Declarações 
var
a : real
b : real
c : real
d : real

inicio
// Seção de Comandos
escreva ("Digite o primeiro valor: ")
leia (a)
c <- (a)
escreva ("Digite o segundo valor: ")
leia (b)
d <- (b)
b <- (c)
a <- (d)
escreva ("O primeiro valor é ", a, " e o segundo é ", b)
fimalgoritmo
